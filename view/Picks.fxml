<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PicksController">
   <children>
      <Group layoutX="5.0" layoutY="-71.0">
         <children>
            <Label layoutX="182.0" layoutY="103.0" text="Draft Picks" underline="true" />
            <TableView fx:id="picksTableView" editable="true" layoutX="58.0" layoutY="126.0" prefHeight="700.0" prefWidth="318.0">
              <columns>
                <TableColumn fx:id="picksPositionColumn" prefWidth="75.0" text="Pos" />
                <TableColumn fx:id="picksNameColumn" prefWidth="75.0" text="Player" />
                <TableColumn fx:id="picksFantasyPointsColumn" prefWidth="75.0" text="Fsy" />
                <TableColumn fx:id="picksDiffColumn" prefWidth="75.0" text="Diff" />
              </columns>
            </TableView>
         </children>
      </Group>
      <Button fx:id="refreshButton" layoutX="458.0" layoutY="353.0" mnemonicParsing="false" onAction="#refreshButtonAction" text="Refresh" />
   </children>
</AnchorPane>
